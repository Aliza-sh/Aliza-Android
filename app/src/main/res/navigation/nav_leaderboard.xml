<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_leaderboard"
    app:startDestination="@id/leaderboard">
    <fragment
        android:id="@+id/userProfile"
        android:name="com.aliza.alizaandroid.ui.FragmentUserProfile"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile" />
    <fragment
        android:id="@+id/leaderboard"
        android:name="com.aliza.alizaandroid.ui.FragmentLeaderboard"
        android:label="fragment_leaderboard"
        tools:layout="@layout/fragment_leaderboard">
        <action
            android:id="@+id/action_leaderboard_to_userProfile"
            app:destination="@id/userProfile" />
    </fragment>
</navigation>